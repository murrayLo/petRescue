<p id="notice"><%= notice %></p>

<h1>Found Reports</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Location</th>
      <th>Animal</th>
      <th>Breed</th>
      <th>Colour</th>
      <th>Image</th>
      <th>Additionalinfo</th>
      <th>Reporter</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @found_reports.each do |found_report| %>
      <tr>
        <td><%= found_report.date %></td>
        <td><%= found_report.location %></td>
        <td><%= found_report.animal %></td>
        <td><%= found_report.breed %></td>
        <td><%= found_report.colour %></td>
        <td><%= found_report.image %></td>
        <td><%= found_report.additionalInfo %></td>
        <td><%= found_report.reporter %></td>
        <td><%= link_to 'Show', found_report %></td>
        <td><%= link_to 'Edit', edit_found_report_path(found_report) %></td>
        <td><%= link_to 'Destroy', found_report, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Found Report', new_found_report_path %>
